{
  "name": "@gabortorma/mwm",
  "type": "module",
  "version": "0.9.0",
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "description": "Multi-repo Workspace Manager",
  "author": "gabortorma",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "github",
    "url": "git+ssh://git@github.com/gabortorma/mwm.git"
  },
  "keywords": [
    "citty",
    "consola",
    "syncpack-alternative",
    "workspace-tools",
    "release-it",
    "pnpm",
    "mono-repo",
    "multi-repo",
    "workspace"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./generate": {
      "types": "./dist/commands/generate/generate.d.ts",
      "import": "./dist/commands/generate/generate.mjs",
      "require": "./dist/commands/generate/generate.cjs"
    },
    "./release": {
      "types": "./dist/commands/release/release.d.ts",
      "import": "./dist/commands/release/release.mjs",
      "require": "./dist/commands/release/release.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mwm": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepack": "unbuild",
    "dev": "pnpm --filter @gabortorma/mwm-playground dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "prerelease": "pnpm lint",
    "release": "dotenv release-it",
    "cli": "jiti ./scripts/cli.js",
    "merge-template": "./scripts/merge-template.sh"
  },
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "c12": "3.0.3",
    "change-case": "^5.4.4",
    "citty": "^0.1.6",
    "compare-versions": "^6.1.1",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "pkg-types": "^2.1.0",
    "pnpm": "^10.9.0",
    "release-it": "^19.0.1",
    "replace-in-file": "^8.3.0",
    "simple-git": "^3.27.0",
    "workspace-tools": "^0.38.4"
  },
  "devDependencies": {
    "@gabortorma/antfu-eslint-config": "^3.0.1",
    "@octokit/types": "^13.10.0",
    "@types/node": "^22.15.2",
    "add": "^2.0.6",
    "dotenv-cli": "^8.0.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.2"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@octokit/rest"
      ]
    }
  }
}
