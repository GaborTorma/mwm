{
  "name": "@gabortorma/mwm",
  "type": "module",
  "version": "0.1.10",
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "description": "Multi-repo Workspace Manager",
  "author": "gabortorma",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+ssh://git@github.com/gabortorma/mwm.git"
  },
  "keywords": [
    "citty",
    "consola",
    "syncpack-alternative",
    "workspace-tools",
    "release-it",
    "pnpm",
    "mono-repo",
    "multi-repo",
    "workspace"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "./release-it-config/package": {
      "types": "./dist/release-it-config/package.d.ts",
      "import": "./dist/release-it-config/package.mjs",
      "require": "./dist/release-it-config/package.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mwm": "./bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "pnpm run build",
    "release": "dotenv release-it",
    "typecheck": "tsc --noEmit",
    "test": "pnpm lint && pnpm typecheck && vitest run",
    "cli": "jiti ./scripts/cli.js",
    "merge-template": "./scripts/merge-template.sh"
  },
  "dependencies": {
    "@pnpm/exec": "^2.0.0",
    "citty": "^0.1.6",
    "compare-versions": "^6.1.1",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "pnpm": "^9.5.0",
    "release-it": "^17.6.0",
    "workspace-tools": "^0.36.4"
  },
  "devDependencies": {
    "@gabortorma/antfu-eslint-config": "^0.5.16",
    "@types/node": "^20.14.11",
    "dotenv-cli": "^7.4.2",
    "jiti": "^1.21.6",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.0.3"
  }
}
